{
  "version": 2,
  "buildCommand": "cd driver-incentive-frontend && pnpm install --frozen-lockfile || npm ci || yarn install && pnpm run build || npm run build || yarn build",
  "functions": {
    "api/**.js": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    { "src": "/api/proxy/(.*)", "dest": "/api/proxy/$1" },
    { "src": "/api/(.*)", "dest": "/api/proxy/$1" },
    { "src": "/assets/(.*)", "dest": "driver-incentive-frontend/dist/assets/$1" },
    { "src": "/favicon.ico", "dest": "driver-incentive-frontend/dist/favicon.ico" },
    { "src": "/(.*)", "dest": "driver-incentive-frontend/dist/index.html" }
  ]
}
